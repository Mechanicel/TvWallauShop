{
  "name": "tvwallaushop",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "contracts",
    "backend",
    "frontend"
  ],
  "scripts": {
    "nx": "nx",
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "lint": "nx run-many -t lint",
    "dev": "nx run-many -t serve --parallel --output-style=stream",
    "stop": "nx run-many -t stop --parallel --output-style=stream",
    "clear": "nx run-many -t clear --parallel --output-style=stream",
    "deps:reset": "nx run-many -t deps:reset --parallel --output-style=stream",
    "graph": "nx graph"
  },
  "devDependencies": {
    "nx": "^19.8.3",
    "rimraf": "^6.0.1"
  }
}
