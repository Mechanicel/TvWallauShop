{
  "name": "tvwallaushop-backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev:start": "node ../scripts/dev/service-runner-node.js start --pid-file target/dev.pid --meta-file target/dev.meta.json -- npm run start:dev",
    "dev:stop": "node ../scripts/dev/service-runner-node.js stop --pid-file target/dev.pid --meta-file target/dev.meta.json",
    "dev:status": "node ../scripts/dev/service-runner-node.js status --pid-file target/dev.pid --meta-file target/dev.meta.json",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn src/index.ts",
    "migrate": "knex migrate:latest --knexfile knexfile.ts --env development",
    "migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "seed": "knex --knexfile knexfile.ts seed:run"
  },
  "dependencies": {
    "@tvwallaushop/contracts": "file:../contracts",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.2.0",
    "nodemailer": "7.0.11",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^7.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
