{
  "name": "tvwallaushop-backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "migrate": "knex migrate:latest --knexfile knexfile.ts --env development",
    "migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "openapi:lint": "node -e \"require('fs').accessSync('contracts/openapi.yaml')\"",
    "openapi:types": "openapi-typescript ./contracts/openapi.yaml -o ./contracts/openapi.d.ts",
    "seed": "knex --knexfile knexfile.ts seed:run",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn src/index.ts"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "multer": "2.0.2",
    "mysql2": "^3.0.0",
    "nodemailer": "7.0.11",
    "pg": "^8.11.0",
    "socket.io": "^4.8.3",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^7.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "openapi-typescript": "^7.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
