{
  "name": "backend",
  "projectType": "application",
  "sourceRoot": "backend/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "backend"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "backend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo \"No lint configured for backend\"",
        "cwd": "backend"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev:start",
        "cwd": "backend"
      }
    },
    "stop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev:stop",
        "cwd": "backend"
      }
    },
    "status": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev:status",
        "cwd": "backend"
      }
    }
  }
}
