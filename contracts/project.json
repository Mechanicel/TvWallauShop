{
  "name": "contracts",
  "projectType": "library",
  "sourceRoot": "contracts/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "contracts"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "contracts"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo \"No lint configured for contracts\"",
        "cwd": "contracts"
      }
    },
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rimraf node_modules dist build coverage .turbo .vite .next out target",
        "cwd": "contracts"
      }
    },
    "deps:reset": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rimraf node_modules package-lock.json",
        "cwd": "contracts"
      }
    }
  }
}
