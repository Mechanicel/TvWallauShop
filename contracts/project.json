{
  "name": "contracts",
  "projectType": "library",
  "sourceRoot": "contracts/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "contracts"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "contracts"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo \"No lint configured for contracts\"",
        "cwd": "contracts"
      }
    }
  }
}
